{
  "agent": {
    "model": "anthropic/claude-sonnet-4-5-20250929",
    "thinkingLevel": "medium"
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace"
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "skills": ["assistant", "coach", "nutrition", "books", "spanish"]
      }
    ]
  },
  "skills": {
    "path": "./skills"
  },
  "session": {
    "scope": "per-sender",
    "dmScope": "main",
    "mainKey": "main",
    "reset": {
      "mode": "daily",
      "atHour": 4
    },
    "resetTriggers": ["/new", "/reset"]
  },
  "gateway": {
    "port": 18789
  },
  "channels": {
    "telegram": {
      "enabled": true
    }
  }
}
