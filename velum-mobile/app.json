{
  "expo": {
    "name": "Velum",
    "slug": "velum-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "velum",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#1c1917",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.velum.mobile",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Velum uses health data to track your fitness metrics."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1c1917"
      },
      "package": "com.velum.mobile"
    },
    "plugins": ["expo-router", "expo-sqlite"],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
